import 'package:get/get.dart';


class FModelController extends GetxController{
  var positionX = 300.0.obs;
  var positionY = 300.0.obs;

  var fbWidth = 100.obs;
  var fbHeight = 50.obs;

  var fbColR = 0.obs;

  incCR() => fbColR += 10;

  incW() => fbWidth += 2;
  decW() => fbWidth -= 2;

  incH() => fbHeight += 2;
  decH() => fbHeight -= 2;
  
}